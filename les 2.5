import datetime


class time_calculation:
  def __init__(self, path):
    self.path = path
    self.create_time = datetime.datetime.now()
    print(self.create_time)

  def __enter__(self):
    self.file = open(self.path, 'w')
    return self.file

  def __exit__(self, exc_type, exc_val, exc_tb):
    self.file.close()
    self.end_time = datetime.datetime.now()
    print(self.end_time)
    print(self.end_time - self.create_time)


if __name__ == '__main__':
  with time_calculation('les_2.5.txt') as f:
    a = [1, 1]
    sum_Fibonacci_numbers = 0
    n = int(input('Введите количество первых чисел Фибоначчи: '))
    for i in range(0, n):
      if i <= 1:
        sum_Fibonacci_numbers += a[i]
        continue
      a.insert(i, 0)
      a[i] = int(a[i - 1]) + int(a[i - 2])
      sum_Fibonacci_numbers += a[i]
    print(sum_Fibonacci_numbers)
